	$('#JExcelTable').jexcel('setValue', 'A1', valueToSet);

get data from excel file:
	html: &lt;input type="file" id="selectedFile" />
	jquery:
	document.getElementById('selectedFile').addEventListener("change", (event) => {
            selectedFile = event.target.files[0];
            if (selectedFile) {
                let fileReader = new FileReader();
                fileReader.readAsBinaryString(selectedFile);
                fileReader.onload = (event) => {
                    let data = event.target.result;
                    let workbook = XLSX.read(data, { type: "binary" });
                    workbook.SheetNames.forEach(sheet => {
                        let rowObject = XLSX.utils.sheet_to_row_object_array(workbook.Sheets[sheet]);
                        jsonContent = rowObject;
                    });
                }
                fileReader.onloadend = (event) => {
                    if (jsonContent) {
                        // data return as List of object in jsonContent
			// to display our data on our jexcel table the excel file must be in the same format of our jexcel table (SAME TITLE NAME)
			$.each(jsonContent, function (index, value) {
                            $('#JExcelTable').jexcel('setValue', 'A' + index, value.Controller);
                            $('#JExcelTable').jexcel('setValue', 'B' + index, value.Action);
                            $('#JExcelTable').jexcel('setValue', 'C' + imdex, value.FieldKey);
                            $('#JExcelTable').jexcel('setValue', 'D' + index, value.English);
                            $('#JExcelTable').jexcel('setValue', 'E' + index, value.Arabic);
                            $('#JExcelTable').jexcel('setValue', 'F' + index, value.French);
                           
                        });
                    }
                }
            }
        });
