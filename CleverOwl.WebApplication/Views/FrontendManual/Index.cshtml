        $("#select2_id").select2({
	        placeholder: "Select a State"
        )};
	2- using AJAX:
	$('#select2_id').select2({
 		ajax: {
    		url: 'url goes here',
    		data: function (params) {
      		var query = { // JSON data passed as parameter in the url function eg:
        		search: params.term,
        		type: 'public'
      		}

      		// Query parameters will be ?search=[term]&type=public
      		return query;
    		}
  		}
	});
	3- Set Language:
	$("#select2_id").select2({
  		language: "es"
	});
	4- Search query:
	var searchInput = $('#select2_id').select2({
        ajax: {
            url: '@@Url.Action("action_name", "controller_name")',
            dataType: 'json',
            type: 'POST',
            delay: 250,
            cache: true,
            allowClear: true,
            multiple: true,
            maximumSelectionLength: 1,
            data: function (params) {
                var query = { // JSON data passed as parameter in the url function eg:
                    query: params.term,
                        __RequestVerificationToken : $('@@Html.AntiForgeryToken()').val() // if needed
                }

                return query;
            },
