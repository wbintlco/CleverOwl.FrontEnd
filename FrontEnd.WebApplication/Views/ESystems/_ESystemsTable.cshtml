@model IEnumerable<RLI.EntityFramework.EDM.ESystem>
@using RLI.Common.Managers
@{
    Dictionary<string, string> Locale = ViewBag.Locale;

}

<div id="CycleTableDiv" class="col-lg-12" style="min-height:320px;">
    <table id="m-table" class="table table-striped- table-bordered table-hover table-checkable dataTable no-footer dtr-inline" role="grid">
        <thead>
            <tr>
                <td></td>
                <td>@Html.Raw(UtilitiesManager.GetFieldLabel(Locale, "ESystemsPageProvider"))</td>
                <td>@Html.Raw(UtilitiesManager.GetFieldLabel(Locale, "ESystemsPageActions"))</td>
            </tr>
        </thead>
        <tbody>
            @foreach (var eSystem in Model)
            {
                <tr>
                    <td>@eSystem.ESystemKey</td>
                    <td>@eSystem.ESystemName</td>
                    <td></td>
                </tr>
            }
        </tbody>
    </table>
</div>




