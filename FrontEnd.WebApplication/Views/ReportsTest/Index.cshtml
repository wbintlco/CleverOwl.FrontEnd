        //function to hide the overflow
        function hideOverflow() {
            $('html, body').css({
                overflow: 'hidden',
                height: '100%'
            });
        }
        //function to reset the overflow
        function resetOverFlow() {
            $('html, body').css({
                overflow: 'auto',
                height: 'auto'
            });
        }

        // funtion to detect the screen width and show the overlay for PC
        function getScreenTypeAndShowOverlay(html) {
            $("#m_aside_header_menu_mobile_toggle").removeClass("m-brand__toggler--active");
            $("#m_header_menu").removeClass("m-aside-header-menu-mobile--on");
            $('#m_aside_left').removeClass("m-aside-left--on");
            $('#tutorial').remove();
            $('#overlay-tutorial').append(html);
            width = $(window).width();
            if (width > 1500) {
                showOverlay('pcL');
            } else if (width > 1023 && width <= 1500) {
                showOverlay('pc');
            } else if (width >= 800 && width < 1024) {
                showOverlay('ipadPro');
            } else if (width >= 600 && width < 800) {
                showOverlay('ipad');
            } else if (width >= 450 && width < 600) {
                showOverlay('phoneL');
            } else if (width < 450) {
                showOverlay('phone');
            }

        }
        var displayOverlay = true;

        $(document).ready(function () {
            @if(currentUser.StatusKey != 19)
            {
                <text>setOverlay();</text>
            }
