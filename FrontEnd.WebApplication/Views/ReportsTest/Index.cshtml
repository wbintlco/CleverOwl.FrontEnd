                chart2.exporting.menu.align = "left";
                chart2.exporting.menu.verticalAlign = "top"

            }); // end am4core.ready()

            $('#applyBtn').on('click', function () {
                var teacher = $('#teachersDL').val();
                var students = $('#studentsDL').val();
                if (teacher != "") {
                    $('.teacherBlock').fadeIn(1000);
                }
                if (students != "") {
                    $('.studentBlock').fadeIn(1000)
                }
            });

            $('#clearBtn').on('click', function () {
                $('#schoolsDL').val(null).trigger('change');
                $('#teachersDL').val(null).trigger('change');
                $('#studentsDL').val(null).trigger('change');
                $('.teacherBlock').fadeOut(1000);
                $('.studentBlock').fadeOut(1000);
            });

        });

</script>

}

