        // function responsible for the mobile screen overlay(small)
        function showMobileOverlay(overlayId, indicatorId, carouselItemId, carouselId, slideNextBtnId, slidePrevBtnId, overlayEndClass, doneBtnId, radioID) {
            console.log('mobile');
            hideOverflow();
            $('#' + overlayId).show();
            $("#m_aside_header_menu_mobile_toggle").addClass("m-brand__toggler--active");
            $("#m_header_menu").addClass("m-aside-header-menu-mobile--on");
            var slide_to_index = 0;
            $('.carousel-inner').css('margin-top', '130px');
            $('#' + slidePrevBtnId).hide();
            $('#' + slideNextBtnId).click(function () {
                if (slide_to_index == 2) {
                    $("#m_aside_header_menu_mobile_toggle").removeClass("m-brand__toggler--active");
                    $("#m_header_menu").removeClass("m-aside-header-menu-mobile--on");
                }
                if (slide_to_index == 1) {
                    $('.carousel-inner').css('margin-top', '160px');
                } else {
                    $('.carousel-inner').css('margin-top', '0px');
                }
                mApp.block($('#' + overlayId));
                $('#' + slidePrevBtnId).show();
                if (slide_to_index < 6) {
                    $('#' + indicatorId + slide_to_index).hide();
                    $('#' + carouselItemId + slide_to_index).removeClass("active");
                    $('#' + carouselId + slide_to_index).removeClass("active");
                    ++slide_to_index;
                    $('#' + carouselItemId + slide_to_index).addClass("active");
                    $('#' + carouselId + slide_to_index).addClass("active");
                    $('#' + indicatorId + slide_to_index).show();
                }
                if (slide_to_index == 5) {
                    $('#m_aside_left').addClass("m-aside-left--on");
                    $('#' + slideNextBtnId).hide();
                    $('.' + overlayEndClass).show();
                }

                mApp.unblock($('#' + overlayId));

