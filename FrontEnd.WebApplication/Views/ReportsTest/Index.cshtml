        function setOverlay() {
            var html_overlay = $('#overlay-tutorial').html();

            $(window).resize(function () {                 // detect each
                getScreenTypeAndShowOverlay(html_overlay);// time the
            });                                          // window changes

            var isMobile = navigator.userAgent.match(/(iPhone|iPod|Android|BlackBerry|Nokia)/); //detect if the user is on a phone
            var isTablet = navigator.userAgent.match(/(iPad)/);                                //detect if the user is on a Tablet or ipad
            var screenType;
            if (isMobile) {
                var width = window.screen.width;
                if (width >= 450) {
                    screenType = 'phoneL';
                } else if (width > 300 && width < 450) {
                    screenType = 'phone';
                } else {
                    screenType = 'phoneS';
                }
                showOverlay(screenType);
            } else if (isTablet) {
                var height = window.screen.height;
                var width = window.screen.width;
                if (width > 768 && height > 1024) {
                    screenType = 'ipadPro';
                } else {
                    screenType = 'ipad';
                }
                showOverlay(screenType);
            } else {
                getScreenTypeAndShowOverlay(html_overlay);
            }
        }

        // function that display the overlay tutorial
        function showOverlay(screenType) {
            if (displayOverlay == true) {
                if (screenType == 'phone') {
