                } else if (screenType == 'phoneL') {
                    showMobileOverlay('overlaySD', 'indicatorSD_', 'carouselSD-item_', 'carouselSD-toggle_', 'slide_next_btn_SD', 'slide_prev_btn_SD', 'closeOverlaySD', 'closeOverlayBtnSD', 'radioSD');
