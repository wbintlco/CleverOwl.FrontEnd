@model IEnumerable<RLI.EntityFramework.EDM.DataType>
@using RLI.Common.Managers
@{
    Dictionary<string, string> Locale = ViewBag.Locale;

}

<div id="DataTypeTableDiv" class="col-lg-12" style="min-height:320px;">
    <table id="m-table" class="table table-striped- table-bordered table-hover table-checkable dataTable no-footer dtr-inline" role="grid">
        <thead>
            <tr>
                <td></td>
                <td>@Html.Raw(UtilitiesManager.GetFieldLabel(Locale, "DataTypesPageDataType"))</td>
                <td>@Html.Raw(UtilitiesManager.GetFieldLabel(Locale, "DataTypesPageActions"))</td>
            </tr>
        </thead>
        <tbody>
            @foreach (var dataType in Model)
            {
                <tr>
                    <td>@dataType.DataTypeKey</td>
                    <td>@dataType.DataType1</td>
                    <td></td>
                </tr>
            }
        </tbody>
    </table>
</div>






