@model IEnumerable<RLI.EntityFramework.EDM.AspNetRole>
@using RLI.Common.Managers
@{
    Dictionary<string, string> Locale = ViewBag.Locale;

}

<div id="AspNetRoleTableDiv" class="col-lg-12" style="min-height:320px;">
    <table id="m-table" class="table table-striped- table-bordered table-hover table-checkable dataTable no-footer dtr-inline" role="grid">
        <thead>
            <tr>
                <td></td>
                <td>@Html.Raw(UtilitiesManager.GetFieldLabel(Locale, "AspNetRolesPageRoleName"))</td>
                <td>@Html.Raw(UtilitiesManager.GetFieldLabel(Locale, "AspNetRolesPageRoleType"))</td>
                <td>@Html.Raw(UtilitiesManager.GetFieldLabel(Locale, "AspNetRolesPageActions"))</td>
            </tr>
        </thead>
        <tbody>
            @foreach (var role in Model)
            {
                <tr>
                    <td>@role.Id</td>
                    <td>@role.Name</td>
                    <td>@role.RoleType.RoleType1</td>
                    <td></td>
                </tr>
            }
        </tbody>
    </table>
</div>